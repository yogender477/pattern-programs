
#include <stdio.h>

int main()
{
    int i,j,n=5,p=1;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("%d",p);
        }
         p=p+2;
        printf("\n");
    }
    p=8;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=n-i;j++)
        {
            printf("%d",p);
        }
        p=p-2;
        printf("\n");
    }

    return 0;
}
